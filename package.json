    "clean": "rm -rf dist && mkdir dist",
    "test": "mocha --compilers js:babel-core/register --require ./app/src/test/helpers --require ./app/src/test/helpers/enzyme.js  ./app/src/test/*.spec.js",
    "build:server": "babel lib -d dist",
    "build:client": "./node_modules/.bin/webpack",
    "build": "npm-run-all build:*",
    "production": "npm run build & cross-env NODE_ENV=production npm run serve"
  },
